generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Song {
  id           BigInt @id @default(autoincrement())
  title        String @db.VarChar(255)
  thumbnailUrl String @db.VarChar(255)
  videoUrl     String @db.VarChar(255)
  duration     Int
}
